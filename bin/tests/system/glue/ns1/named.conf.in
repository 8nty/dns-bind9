options {
	query-source address 10.53.0.1;
	notify-source 10.53.0.1;
	transfer-source 10.53.0.1;
	port @PORT@;
	pid-file "named.pid";
	listen-on { 10.53.0.1; };
	listen-on-v6 { none; };
	recursion yes;
	notify no;
};

zone "." {
	type primary;
	file "root.db";
};

zone "root-servers.nil" {
	type primary;
	file "root-servers.nil.db";
};
zone "net" {
	type primary;
	file "net.db";
};
