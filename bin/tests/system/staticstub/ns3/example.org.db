$ORIGIN .
$TTL 300	; 5 minutes
example.org		IN SOA	ns.example.org. hostmaster.example.org. (
				2010080906 ; serial
				20         ; refresh (20 seconds)
				20         ; retry (20 seconds)
				1814400    ; expire (3 weeks)
				3600       ; minimum (1 hour)
				)
example.org.		NS	ns.example.org.
ns.example.org.		A	10.53.0.3

data.example.org.	TXT	"example org data"
