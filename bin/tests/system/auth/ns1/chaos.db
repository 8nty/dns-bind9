@			CH SOA  ns root (
				2018010100 ; serial
				1800       ; refresh (30 minutes)
				1800       ; retry (30 minutes)
				1814400    ; expire (3 weeks)
				3600       ; minimum (1 hour)
				)
			NS	ns
ns			A	ch-addr.example. 1001
test 			A	ch-addr.example. 1002
			A	ch-addr.example. 1003
