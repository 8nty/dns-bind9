options {
	query-source address 10.53.0.7 dscp 1;
	notify-source 10.53.0.7 dscp 2;
	transfer-source 10.53.0.7 dscp 3;
	port @PORT@;
	pid-file "named.pid";
	listen-on { 10.53.0.7; };
	listen-on-v6 { none; };
	recursion no;
	answer-cookie no;
	send-cookie yes;
	nocookie-udp-size 512;
};

zone "." {
	type primary;
	file "root.db";
};
