options {
	query-source address 10.53.0.1;
	notify-source 10.53.0.1;
	transfer-source 10.53.0.1;
	port @PORT@;
	pid-file "named.pid";
	listen-on { 10.53.0.1; };
	listen-on-v6 { none; };
	recursion no;
	notify yes;
};

zone "include" {
	type primary;
	file "include.db";
};

zone "ttl1" {
	type primary;
	file "ttl1.db";
};

zone "ttl2" {
	type primary;
	file "ttl2.db";
};
