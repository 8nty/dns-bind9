options {
	port @PORT@;
	pid-file "named.pid";
	listen-on { 10.53.0.4; };
	listen-on-v6 { none; };
	recursion yes;
    dnssec-validation yes;
};

view normal {
	match-clients { any; };
};

view "view with a space" {
	match-clients { none; };
};
