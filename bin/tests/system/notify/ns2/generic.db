$TTL 300	; 5 minutes
@			IN SOA	mname1. . (
				1          ; serial
				300        ; refresh (300 seconds)
				300        ; retry (300 seconds)
				1814400    ; expire (3 weeks)
				3600       ; minimum (1 hour)
				)
        		NS	ns2
        		NS	ns3
ns2     		A	10.53.0.2
ns3     		A	10.53.0.3

a			A	10.0.0.1
