options {
	dscp 46;
	query-source address 10.53.0.2;
	notify-source 10.53.0.2;
	transfer-source 10.53.0.2;
	port @PORT@;
	pid-file "named.pid";
	listen-on { 10.53.0.2; };
	listen-on-v6 { none; };
	recursion no;
	dnssec-validation no;
	notify yes;
};

zone "." {
	type secondary;
	file "root.bk";
	primaries { 10.53.0.1; };
};
