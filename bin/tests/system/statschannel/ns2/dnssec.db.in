$ORIGIN .
$TTL 300

dnssec.			IN SOA	mname1. . (
				1          ; serial
				20         ; refresh (20 seconds)
				20         ; retry (20 seconds)
				1814400    ; expire (3 weeks)
				3600       ; minimum (1 hour)
				)
dnssec.			NS	ns2.dnssec.
ns2.dnssec.		A	10.53.0.2

$ORIGIN dnssec.
a			A	10.0.0.1
			MX	10 mail.dnssec.
mail			A	10.0.0.2
