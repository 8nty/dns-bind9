$ORIGIN .
$TTL 300

manykeys.			IN SOA	mname1. . (
				1          ; serial
				20         ; refresh (20 seconds)
				20         ; retry (20 seconds)
				1814400    ; expire (3 weeks)
				3600       ; minimum (1 hour)
				)
manykeys.			NS	ns2.manykeys.
ns2.manykeys.		A	10.53.0.2

$ORIGIN manykeys.
a			A	10.0.0.1
			MX	10 mail.manykeys.
mail			A	10.0.0.2
