options {
	query-source address 10.53.0.3;
	notify-source 10.53.0.3;
	transfer-source 10.53.0.3;
	port @PORT@;
	pid-file "named.pid";
	listen-on { 10.53.0.3; };
	listen-on-v6 { none; };
	recursion no;
	notify no;
};

view "in" {
	zone "example." {
		type secondary;
		primaries { 10.53.0.1; };
		inline-signing yes;
		auto-dnssec maintain;
		file "example-in.bk";
	};
};
