options {
	query-source address 10.53.0.5;
	notify-source 10.53.0.5;
	transfer-source 10.53.0.5;
	port @PORT@;
	pid-file "named.pid";
	listen-on { 10.53.0.5; };
	listen-on-v6 { none; };
	forward only;
	forwarders { 10.53.0.4; };
	deny-answer-aliases { "rebind"; };
	dnssec-validation yes;
};

zone "." {
	type hint;
	file "root.db";
};

zone "rebind" {
	type primary;
	file "rebind.db";
};
