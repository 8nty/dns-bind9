key one {
	algorithm hmac-md5;
	secret "1234abcd8765";
};

options {
	port @PORT@;
	pid-file "named.pid";
	listen-on { 10.53.0.2; };
	listen-on-v6 { none; };
	recursion no;
	allow-query {! key one; };
};

include "controls.conf";

zone "." {
	type hint;
	file "../../common/root.hint";
};

zone "normal.example" {
	type primary;
	file "generic.db";
};
