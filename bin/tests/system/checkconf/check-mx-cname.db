$TTL 600	; 10 minutes
@		IN SOA	mname1. . (
				1          ; serial
				20         ; refresh (20 seconds)
				20         ; retry (20 seconds)
				1814400    ; expire (3 weeks)
				3600       ; minimum (1 hour)
				)
			NS	ns2
			MX	10 mail

; MX points to a CNAME which is detected by check-mx-cname
mail			CNAME	ns2

ns2			A	10.53.0.2
