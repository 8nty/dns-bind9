$TTL 600	; 10 minutes
@		IN SOA	mname1. . (
				1          ; serial
				20         ; refresh (20 seconds)
				20         ; retry (20 seconds)
				1814400    ; expire (3 weeks)
				3600       ; minimum (1 hour)
				)
			NS	ns2
; MX appears to be an address and will be detected by check-mx
			MX	10 10.0.0.1

ns2			A	10.53.0.2
