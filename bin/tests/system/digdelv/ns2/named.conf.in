// NS2

options {
	query-source address 10.53.0.2;
	port @PORT@;
	pid-file "named.pid";
	listen-on { 10.53.0.2; };
	listen-on-v6 { fd92:7065:b8e:ffff::2; };
	recursion no;
	dnssec-validation no;
};

zone "." {
	type hint;
	file "../../common/root.hint";
};

zone "example" {
	type primary;
	file "example.db";
};
