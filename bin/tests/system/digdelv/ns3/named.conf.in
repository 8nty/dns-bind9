options {
	query-source address 10.53.0.3 dscp 1;
	port @PORT@;
	pid-file "named.pid";
	listen-on { 10.53.0.3; };
	listen-on-v6 { fd92:7065:b8e:ffff::3; };
	recursion yes;
	dnssec-validation no;
	server-id "ns3";
};

zone "." {
	type hint;
	file "../../common/root.hint";
};
