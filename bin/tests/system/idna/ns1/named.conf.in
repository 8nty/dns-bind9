// NS1

options {
	query-source address 10.53.0.1;
	port @PORT@;
	pid-file "named.pid";
	listen-on { 10.53.0.1; };
	listen-on-v6 { fd92:7065:b8e:ffff::1; };
	recursion no;
	notify yes;
	dnssec-validation no;
};

zone "." {
	type primary;
	file "root.db";
};
